<mapper namespace="com.Joonggodeal.joonggodeal.Admin.SignInUp.Dao.AdminLoginDao">

    <select id="userIdCheck" parameterType="String" resultType="int">
        SELECT COUNT(admin_id)
        FROM joonggodeal.admin_info
        WHERE admin_id = #{id}
    </select>

    <select id="getUserPw" parameterType="HashMap" resultType="String">
        SELECT admin_pw
        FROM joonggodeal.admin_info
        WHERE admin_id = #{id}
    </select>



</mapper>